SELECT latlong, 
'<a href="person?id=', '">',ownername, '<a/><br />',
owneraddress , '<br>', 
emailaddress, ' ', mobiletelephone
FROM animallost
INNER JOIN owner ON animallost.OwnerID = owner.ID
WHERE datefound is null AND latlong is not null AND latlong <> ''
ORDER BY ownername
